{
    "manifest_version": 2,
    "version": "1.0",
    "name": "Sce Url Opener",
    
    
    "description": "Open all urls in sce moodle page",
    
    "icons" : {
        "128" : "images/SCE_logo.png",
        "64" : "images/SCE_logo.png",
        "32" : "images/SCE_logo.png",
        "16" : "images/SCE_logo.png"
    },
    
    "browser_action" : {
        "default_icon": "images/SCE_logo.png",
        "default_title": "Open all urls in sce moodle page"
    },

    "permissions" : [
        "http://*/*",
        "https://*/*",
		"activeTab",
		"tabs"
    ],
	
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    }
}